{
  "name": "@ppey/react-increasable",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p-pey/React-increasable.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "increasable",
    "react-increasable",
    "react-add-component",
    "react-increasable-component"
  ],
  "author": "Pooriya Peyvand (Pooriya-Peyvand)",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepublishOnly": "npm run prettier && npm run lint"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "prettier": "2.8.7",
    "eslint": "8.36.0",
    "@vitejs/plugin-react": "^3.1.0",
    "husky": "^8.0.3",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.7"
  }
}
